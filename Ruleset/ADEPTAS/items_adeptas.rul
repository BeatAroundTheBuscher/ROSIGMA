extended:
  tags:
    RuleItem:
      ITEM_COLOR_CHANGES_WITH_AMMO_NON_GRAY: int
      ITEM_AMMO_COLOR: int
      ITEM_AMMO_SHADE: int
      ITEM_STRENGTH_REQUIREMENT: int
      ITEM_AUTO_FLAT_POWER_BONUS: int
