      - EXO_CORPSE1
      - EXO_CORPSE2
      - EXO_CORPSE3
      - EXO_CORPSE4
    frontArmor: 130
    sideArmor: 130
    rearArmor: 130
    underArmor: 100
    size: 2
    damageModifier: #VEHICLE ARMOR LARGE
      - 1.0 #none
      - 1.0 #AP
      - 0.4 #FLAMES
      - 1.0 #HE
      - 1.4 #LASCANON
      - 1.0 #PLASMA
      - 0.0 #STUN
      - 0.8 #MELEE
      - 1.0 #ACID
      - 0.0 #SMOKE
      - 1.0 #IMPACT
      - 1.2 #MELTA
    loftempsSet: [ 92, 89, 90, 91 ]
    drawingRoutine: 3
    scripts:
      selectUnitSprite:
        var int aiming;
        var int frame;

        unit.isAiming aiming;

        if or eq blit_part blit_large_torso_0 eq blit_part blit_large_torso_1 eq blit_part blit_large_torso_2 eq blit_part blit_large_torso_3;

          if neq 0 aiming;

            set sprite_offset 0;
            set frame anim_frame;
            mod frame 4;
            add sprite_offset frame;

          else;

            set sprite_offset 0;

          end;

        else or eq blit_part blit_large_propulsion_0 eq blit_part blit_large_propulsion_1 eq blit_part blit_large_propulsion_2 eq blit_part blit_large_propulsion_3;

          set sprite_index 32;
          set sprite_offset 0;

        end;

        add sprite_index sprite_offset;

        return sprite_index;
