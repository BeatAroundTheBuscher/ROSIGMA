    commands:
    - type: addBlock
      groups: 2
      executions: 2
      rects:
        - [4,4,2,2]
    - type: addBlock
      groups: 4
      size: 2
      rects:
        - [0,0,2,2]

    - type: addUFO
      UFOName: STR_DEFILER_SINGLE_UFO
      size: 2
      blocks: [5]
      rects:
        - [2,2,2,2]
    - type: removeBlock
