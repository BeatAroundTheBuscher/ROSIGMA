    armorTransformations: !add
      STR_GUARD_FLAK_VOX: STR_GUARD_FLAK_VOX_NOAIRSTRIKE
      STR_GUARD_CARAPACE_VOX: STR_GUARD_CARAPACE_VOX_NOAIRSTRIKE
      STR_GUARD_VETERAN_VOX: STR_GUARD_VETERAN_VOX_NOAIRSTRIKE
      STR_GUARD_SCION_VOX: STR_GUARD_SCION_VOX_NOAIRSTRIKE
  - type: STR_INSIDE_ENVIRO     #INSIDE
    armorTransformations: !add
      STR_GUARD_FLAK_VOX: STR_GUARD_FLAK_VOX_NOAIRSTRIKE
      STR_GUARD_CARAPACE_VOX: STR_GUARD_CARAPACE_VOX_NOAIRSTRIKE
      STR_GUARD_VETERAN_VOX: STR_GUARD_VETERAN_VOX_NOAIRSTRIKE
      STR_GUARD_SCION_VOX: STR_GUARD_SCION_VOX_NOAIRSTRIKE
