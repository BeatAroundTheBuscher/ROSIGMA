      - STR_ADEPTAS_ARMOR_ELOHIM
      - STR_ADEPTAS_ARMOR_RETRIBUTOR
      - STR_ADEPTAS_BLOODYROSE
      - STR_ADEPTAS_ARMOR_SUPERIOR
      - STR_ASSASSINNEUTRAL_FEMSUIT
      - STR_ADEPTAS_CALLIDUS_SUIT

enviroEffects:
