  - type: STR_SLAANESH_DEMONPRINCESSARMOR     #Demon Princess
    visibilityAtDay: 40
    visibilityAtDark: 30
    antiCamouflageAtDay: 15
    antiCamouflageAtDark: 15
    heatVision: 30
#    psiVision: 15 # either mind control or psiVision, not both
    tags:
      #ARMOR_ENERGY_SHIELD_CAPACITY: 180
      #ARMOR_ENERGY_SHIELD_PER_TURN: 20
      #ARMOR_ENERGY_SHIELD_FLASH_COLOR: 12
      ARMOR_ENERGY_SHIELD_HP_PER_TURN: 150
      ARMOR_ENERGY_SHIELD_DECAY: 75
      ARMOR_GAINS_PSISKILL_SECONDARY_EXPERIENCE: 6
    bleedImmune: true
    specialWeapon: ALIEN_PSI_WEAPON
    spriteSheet: DEMONPRINCESS.PCK
    spriteInv: ADaemonPrincessS.SPK
    customArmorPreviewIndex: [73, 74, 75]
    scripts:
      selectUnitSprite: |
        var int temp;
        var int walking;
        var int floating;
        unit.isWalking walking;
        unit.isFloating floating;
        if and eq walking 0 eq floating 1;
          unit.getId temp;
          offsetmod temp 11 0 8; #desync animation of different units
          add temp anim_frame;
          wavegen_saw temp 4 4 4;
          add sprite_offset 1;
          add sprite_offset temp;
          add sprite_index sprite_offset;
        else;
          add sprite_index sprite_offset;
        end;
        return sprite_index;
    allowInv: true
    spriteFaceGroup: 6
    spriteFaceColor: [96, 96, 96, 96, 160, 160, 163, 163, 96, 96, 96, 96, 96, 96] #M0 F0 M1 F1 M2 F2 M3 F3 M4 F4 M5 F5 M6 F6
