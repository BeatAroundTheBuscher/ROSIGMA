    bleedImmune: true
    specialWeapon: ALIEN_PSI_WEAPON
    spriteSheet: DEMONPRINCESS.PCK
    spriteInv: ADaemonPrincessS.SPK
    customArmorPreviewIndex: [73, 74, 75]
    scripts:
      selectUnitSprite: |
        var int temp;
        var int walking;
        var int floating;
        unit.isWalking walking;
        unit.isFloating floating;
        if and eq walking 0 eq floating 1;
          unit.getId temp;
          offsetmod temp 11 0 8; #desync animation of different units
          add temp anim_frame;
          wavegen_saw temp 4 4 4;
          add sprite_offset 1;
          add sprite_offset temp;
          add sprite_index sprite_offset;
        else;
          add sprite_index sprite_offset;
        end;
        return sprite_index;
    allowInv: true
    corpseGeo: SLAANSUP_CORPSE
    corpseBattle:
      - SLAANSUP_CORPSE1
      - SLAANSUP_CORPSE2
      - SLAANSUP_CORPSE3
      - SLAANSUP_CORPSE4
    stats:
       tu: 20
       stamina: 40
       health: 50
       bravery: 20 #always multiples of 10
       reactions: 10
       firing: 10
       throwing: 10
       strength: 30
       psiStrength: 30
       psiSkill: 0
       melee: 10
    frontArmor: 140
    sideArmor: 140
    rearArmor: 100
    underArmor: 100
    drawingRoutine: 20
    constantAnimation: false
    movementType: 1
    size: 2
    moveSound: {mod: 40k, index: 828}
