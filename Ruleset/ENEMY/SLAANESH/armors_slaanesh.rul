    corpseBattle:
      - SLAANSUP_CORPSE1
      - SLAANSUP_CORPSE2
      - SLAANSUP_CORPSE3
      - SLAANSUP_CORPSE4
    stats:
       tu: 20
       stamina: 40
       health: 50
       bravery: 20 #always multiples of 10
       reactions: 10
       firing: 10
       throwing: 10
       strength: 30
       psiStrength: 30
       psiSkill: 0
       melee: 10
    frontArmor: 140
    sideArmor: 140
    rearArmor: 100
    underArmor: 100
    drawingRoutine: 20
