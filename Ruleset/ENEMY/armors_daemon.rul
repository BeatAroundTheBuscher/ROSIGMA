      - 0.5 #HE
      - 1.0 #LASCANON
      - 1.1 #PLASMA
      - 0.5 #STUN
      - 0.5 #MELEE
      - 0.0 #ACID
      - 0.0 #SMOKE
      - 0.5 #IMPACT
      - 1.2 #MELTA
    loftempsSet: [ 3 ]
    allowsRunning: false #NPC limits for BAI
