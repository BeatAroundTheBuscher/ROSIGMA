# Mission scripts have been sorted by the type of missions they generate, listed below with search codes.
# There are subcategories denoted by comments for missions belonging to specific faction campaigns, etc.
#
# MISSION SCRIPT TYPE     DESCRIPTION                                   SEARCH CODE
#
# Game Start              Runs only at game start to populate geoscape  #START#
# Bases                   Generates extra bases as the game continues   #BASE#
# Warp Storm              Runs monthly, generic missions that make      #STORM#
#                           ships fly around and establish alien bases
# General Enemy Activity  Other general geoscape activity.              #GEN#
# Terror Missions         Monthly recurring terror missions             #TERROR#
# Space Hulk Missions     Missions that spawn space hulks               #HULK#
# Monthly Missions        General Monthly missions                      #MONTH#
# General Story/Event     Non-faction specific special missions         #SPECIAL#
# Training Missions       Missions triggered by having the training     #TRAIN#
#                           mission research/having gone on a training
#                           mission.
# Faction Specific        Special campaign missions for specific        #CAMP#
#   Campaign Missions       factions. Includes final mission scripts.

adhocScripts:
### Adhoc Game Start Scripts #START#
  # Geoscape initialization for the first month is now triggered via adhocScripts to allow for
  # different factions having completely different starting conditions.
  # This means each strategy _needs_ to have an event associated with it, triggered via
  # spawnedEvent on their strategy research.

  - &STR_START_MISSION
    type: adhocGameStart
    # initial mission in player region
    adhocMissionScriptTags: ["gameStart"]
    missionWeights:
      0:
        STR_ALIEN_RESEARCH: 33
        STR_ALIEN_HARVEST: 33
        STR_ALIEN_ABDUCTION: 33
        STR_NURGLE_RESEARCH: 5
        STR_NURGLE_HARVEST: 15
        STR_NURGLE_ABDUCTION: 5
        STR_SLAANESH_ABDUCTION: 20
        STR_SLAANESH_HARVEST: 8
        STR_ALIEN_RESEARCH_TZEENTCH: 15
        STR_ALIEN_HARVEST_TZEENTCH: 8
    targetBaseOdds: 100
    startDelay: 4320  # 3 days
    randomDelay: 4320  # additional random 0 to 3 days
    useTable: true

  - type: adhocGameStartHard
    refNode: *STR_START_MISSION
    adhocMissionScriptTags: ["gameStartHard"]
    missionWeights:
      0:
        STR_ALIEN_RESEARCH_HARD: 33
        STR_ALIEN_ABDUCTION_HARD: 33
        STR_ALIEN_HARVEST_HARD: 33
        STR_NURGLE_RESEARCH_HARD: 8
        STR_NURGLE_HARVEST_HARD: 20
        STR_NURGLE_ABDUCTION_HARD: 8
        STR_SLAANESH_ABDUCTION_HARD: 20
        STR_SLAANESH_HARVEST_HARD: 8 # rare
        STR_ALIEN_RESEARCH_TZEENTCH_HARD: 15
        STR_ALIEN_HARVEST_TZEENTCH_HARD: 5
        STR_ALIEN_ABDUCTION_TZEENTCH_HARD: 5

  - type: adhocGameStartExtraHard
    refNode: *STR_START_MISSION
    adhocMissionScriptTags: ["gameStartExtraHard"]
    missionWeights:
      0:
        STR_ALIEN_RESEARCH_EXTRA_HARD: 33
        STR_ALIEN_ABDUCTION_EXTRA_HARD: 33
        STR_ALIEN_HARVEST_EXTRA_HARD: 33
        STR_NURGLE_RESEARCH_EXTRA_HARD: 8
        STR_NURGLE_HARVEST_EXTRA_HARD: 20
        STR_NURGLE_ABDUCTION_EXTRA_HARD: 8
        STR_SLAANESH_ABDUCTION_EXTRA_HARD: 20
        STR_SLAANESH_HARVEST_EXTRA_HARD: 8 # rare
        STR_ALIEN_RESEARCH_TZEENTCH_EXTRA_HARD: 15
        STR_ALIEN_HARVEST_TZEENTCH_EXTRA_HARD: 5
        STR_ALIEN_ABDUCTION_TZEENTCH_EXTRA_HARD: 5

  - type: adhocGameStartHard_SW
