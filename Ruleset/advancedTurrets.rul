    newTurnUnit:
      - offset: 3
        code: |

          var int flamersponsonunit;
          var int boltersponsonunit;
          var int mmsponsonunit;
          var int sponson;
          var int run;
          var int y;
          var int x;
          var int z;
          var int temp;
          var int id;
          var ptr BattleUnit other_sponson;
          var ptr Tile other_tile;

          unit.getTag flamersponsonunit Tag.FlamerSponsonUnit;
          if eq flamersponsonunit 1;
            set run 1;
          end;
          unit.getTag boltersponsonunit Tag.BolterSponsonUnit;
          if eq boltersponsonunit 1;
            set run 1;
          end;
          unit.getTag mmsponsonunit Tag.MMSponsonUnit;
          if eq mmsponsonunit 1;
            set run 1;
          end;
          unit.getTag sponson Tag.Sponson;
          if eq sponson 1;
            set run 1;
          end;
          if eq run 1;
            unit.getPosition.getY y;
            unit.getPosition.getX x;
            unit.getPosition.getZ z;
            add x 5;
            battle_game.getTile other_tile x y z;
            other_tile.getUnit other_sponson;
            other_sponson.getTag flamersponsonunit Tag.FlamerSponsonUnit;
            if eq flamersponsonunit 1;
              set temp 1;
            end;
            other_sponson.getTag boltersponsonunit Tag.BolterSponsonUnit;
            if eq boltersponsonunit 1;
              set temp 1;
            end;
            other_sponson.getTag mmsponsonunit Tag.MMSponsonUnit;
            if eq mmsponsonunit 1;
              set temp 1;
            end;
            other_sponson.getTag sponson Tag.Sponson;
            if eq sponson 1;
              set temp 1;
            end;
            if eq temp 1;
              unit.setTag Tag.ReSprite 1;
            end;
          end;
          return;

    selectUnitSprite:
      - offset: 1
        code: |
          var int resprite;

          unit.getTag resprite Tag.ReSprite;
          if eq 1 resprite;
            add sprite_index 100;
          end;
          return sprite_index;


