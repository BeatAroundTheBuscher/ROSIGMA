          var int noofhullbolters;
          var int hulllas;
          var int noofhulllas;
          var int noofspawns;
          var int nooflemanspawns;
          var int noofsponsonspawns;
          var int noofhullspawns;

          item.getTag lasturret Tag.LasTurret;
          if eq 1 lasturret;
            battle_game.getTag nooflasturrets Tag.NoOfLasTurrets;
            battle_game.getTag noofspawns Tag.NoOfSpawns;
            add nooflasturrets 1;
            add noofspawns 1;
            if gt noofspawns 3;
              set noofspawns 3;
            end;
            battle_game.setTag Tag.NoOfLasTurrets nooflasturrets;
            battle_game.setTag Tag.NoOfSpawns noofspawns;
          end;

