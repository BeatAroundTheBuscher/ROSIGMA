  # Used for Spacewolves Razorbacks/Chimeras
  - type: AUX_HELFROST_TWINLINKED_CANNON
    tags:
      TURRET_RELOAD_TURNS: 2
    weight: 0
    size: 0
    invWidth: 2
    invHeight: 3
    bigSprite: 2578
    bulletSprite: {mod: 40k, index: 12}
    fireSound: 2494
    explosionSpeed: 5
    bulletSpeed: 40
    hitAnimation: 140
    hitSound: 2496
    costAuto:
      energy: 5
      time: 1
      mana: 10
    costAimed:
      time: 1
      energy: 10
      mana: 20
    flatUse:
      time: true
      energy: true
    dropoff: 6 #high drop off
    autoRange: 15 #-2 vs bolter range spray mode
    snapRange: 0
    aimRange: 25 #lascannon-like focused mode, -5 range vs lascannon aimed
    accuracyAuto: 40
    accuracySnap: 0
    accuracyAimed: 80 #-10 vs Lascannon aimed
    tuSnap: 0
    power: 100  #Does mostly utility damage
    damageAlter:  # DA BOLTER
      RandomType: 6
      ToArmorPre: 0.3 #Armor Shattering
      ArmorEffectiveness: 0.6 #Low Armor Effectiveness
      ToArmor: 0.15
      ToHealth: 0.3 #Low To Health Damage
      ToStun: 0.2
      ToTime: 0.4 #40% TU damage
      ToEnergy: 0.3
      ToWound: 0.1
      RandomWound: false
    autoShots: 10
    battleType: 1
    fixedWeapon: true
    vaporColorSurface: {mod: 40k, index: 2}
    vaporDensitySurface: 3
    vaporProbabilitySurface: 50
    scripts:
      vaporParticleAmmo: |
        # noting
        set particle_lifetime 20;
        set subvoxel_drift 3;
        return;

