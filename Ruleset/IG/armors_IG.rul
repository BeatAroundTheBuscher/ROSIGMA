  - &STR_GUARD_ARMOR_MOUNTED
    type: STR_GUARD_ARMORS_MOUNTED_LASCANNON_UC
    visibilityAtDay: 40
    visibilityAtDark: 9
    frontArmor: 130
    sideArmor: 30
    rearArmor: 30
    underArmor: 30
    weight: -110
    damageModifier: #VEHICLE ARMOR LARGE
      - 1.0 #none
      - 1.0 #AP
      - 0.8 #FLAMES
      - 1.0 #HE
      - 1.4 #LASCANON
      - 1.0 #PLASMA
      - 0.9 #STUN
      - 0.8 #MELEE
      - 1.0 #ACID
      - 1.0 #SMOKE
      - 0.0 #IMPACT
      - 1.2 #MELTA
    stats:
      tu: -15
      firing: 20
    recovery:
      energy:
        energyRegen: 0.0
        flatHundred: 1.0
    scripts:
      hitUnit:
        var int unitDirection;
        var int attackerDirection;

        if or eq attacker null eq side SIDE_UNDER;

          return power part side;

        end;

        unit.getDirection unitDirection;
        attacker.getDirection attackerDirection;
        add attackerDirection 4;
        mod attackerDirection 8;
        sub unitDirection attackerDirection;
        abs unitDirection;

        if lt unitDirection 2;

          set side SIDE_FRONT;

        end;

        return power part side;
    allowsKneeling: false

  - type: STR_GUARD_ARMORS_MOUNTED_AUTOCANNON_UC
    refNode: *STR_GUARD_ARMOR_MOUNTED

