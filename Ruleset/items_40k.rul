  - type: STR_VALKIA_SHIELD ## Equip to the right hand to reduce damage
    categories: [STR_CAT_MELEE, STR_CAT_ASSAULT]
    size: 0.2
    scripts:
      selectItemSprite: |

        var int temp;
        var int numOfFrames;
        set numOfFrames 4;

        if or eq blit_part blit_item_lefthand eq blit_part blit_item_righthand;

            add temp anim_frame;
            mod temp numOfFrames;
            mul temp 8;
            add sprite_index temp;
        end;

        add sprite_index sprite_offset; # add direction
        return sprite_index;
    costSell: 15000
    weight: 15
    bigSprite: 2329
    floorSprite: 2083
    handSprite: 2470
    clipSize: -1
    hidePower: true
    supportedInventorySections:
      - STR_LEFT_HAND
      - STR_GROUND
    power: 90
    damageType: 7
    accuracyMelee: 110
    tuMelee: 20
    damageAlter: #DA DAEMON OMNOM
      RandomType: 0
      ToArmorPre: 0.6
      ToEnergy: 1.5
      ToWound: 0.1
    twoHanded: false
    #fixedWeapon: true
    recover: false
    skillApplied: true
    strengthApplied: false
    isConsumable: false
    battleType: 3
    invWidth: 2
    invHeight: 3
    armor: 80
    attraction: 1
    listOrder: 12999

