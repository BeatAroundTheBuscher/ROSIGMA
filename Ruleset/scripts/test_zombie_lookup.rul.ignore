            else eq myArmorType "STR_TSARGENT_UC";
              rules.getRuleUnit myRuleUnit "STR_NURGLE_TERMINATOR";
            end;
          end;



            unit.setSpawnUnit myRuleUnit;
            unit.setSpawnUnitInstantRespawn 1;
          return;