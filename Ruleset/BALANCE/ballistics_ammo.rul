    damageAlter: #DA BOLTER
      ToArmorPre: 0.05
      ArmorEffectiveness: 0.9
      ToArmor: 0.1
      ToHealth: 1.1
      ToStun: 0.5
    power: 60
    damageType: 1

  - &STR_BOLTER_AMMO_METAL_STORM
